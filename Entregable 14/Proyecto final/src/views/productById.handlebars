<h4>The product is this</h4>

    <table class="table table-striped">
        <thead>
          <tr>
            <th scope="col"><br>#Id</th>
            <th scope="col">Code</th>
            <th scope="col">Name</th>
            <th scope="col">Description</th>
            <th scope="col">Price</th>
            <th scope="col">Stock</th>
            <th scope="col">Thumbnail</th>
          </tr>
        </thead>
    
        <tbody>
          <tr>
            <th scope="row">{{productid.id}}</th>
            <td>{{productid.code}} </td>
            <td>{{productid.name}} </td>
            <td>{{productid.description}} </td>
            <td>{{productid.price}} </td>
            <td>{{productid.stock}} </td>
            <td> <img src="{{productid.thumbnail}}" alt="El enlace no esta disponible" width="60"></td>
          </tr>
          </tbody>
        </table>
<a href="http://localhost:8080/" type="button" class="btn btn-outline-primary">Back Form</button></a>
